{
  "name": "netzmacht/html",
  "type": "library",
  "description": "Simple library for html rendering",
  "keywords": [
    "Html",
    "helper"
  ],
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "David Molineus",
      "email": "mail@netzmacht.de",
      "homepage": "http://www.netzmacht.de",
      "role": "Project leader"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0",
    "symfony/config": "<4.4 || >= 6.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "doctrine/coding-standard": "^8.0",
    "phpcq/runner-bootstrap": "^1.0@dev",
    "phpspec/phpspec": "~7"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.1.x-dev",
      "dev-master": "2.0.x-dev"
    },
    "contao-manager-plugin": "Netzmacht\\Html\\Infrastructure\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Netzmacht\\Html\\": "src/"
    }
  },
  "support": {
    "email": "mail@netzmacht.de",
    "issues": "https://github.com/netzmacht/html/issues",
    "source": "https://github.com/netzmacht/html"
  }
}
